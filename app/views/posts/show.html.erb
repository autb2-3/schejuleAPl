 <h3>スケジュール詳細</h3>
 
    <table>
      <tr>
        <th>タイトル</th>
        <td><%= @post.title %></td>
      </tr>
      <tr>
        <th>開始日</th>
        <td><%= @post.start_at %></td>
      </tr>
      <tr>
        <th>投稿日時</th>
        <td><%= @post.end_at %></td>
      </tr>
      <tr>
        <th>終日</th>
        <td><%= @post.allday %></td>
      </tr>
      <tr>
        <th>メモ</th>
        <td><%= @post.memo %></td>
      </tr>
    </table>
 
    <div>
      <ul>
        <li class="new_post"><%= link_to "スケジュール一覧に戻る",  posts_path, data: {"turbolinks" => false}%></li>
      </ul>
    </div>